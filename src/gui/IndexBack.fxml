<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" styleClass="background" stylesheets="@CSS.css" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.IndexBackController">
   <children>
      <ImageView fitHeight="83.0" fitWidth="76.0" layoutX="250.0" layoutY="37.0" pickOnBounds="true" preserveRatio="true" styleClass="context-menu">
         <image>
            <Image url="@favicon2.png" />
         </image>
      </ImageView>
      <Button fx:id="fxliste" layoutX="89.0" layoutY="190.0" mnemonicParsing="false" onAction="#handleListeBtn" prefHeight="33.0" prefWidth="161.0" stylesheets="@CSS.css" text="Liste Articles" />
      <Button fx:id="fxajout" layoutX="345.0" layoutY="190.0" mnemonicParsing="false" onAction="#handleAddBtn" prefHeight="33.0" prefWidth="167.0" stylesheets="@CSS.css" text="Ajouter Article" />
      <Button fx:id="fxsupprimer" layoutX="89.0" layoutY="260.0" mnemonicParsing="false" onAction="#handleDeleteBtn" stylesheets="@CSS.css" text="Supprimer Article" />
      <Label layoutX="195.0" layoutY="140.0" styleClass="label-bright" stylesheets="@CSS.css" text="AIDME BLOG DASHBOARD ">
         <font>
            <Font name="Baskerville Old Face" size="8.0" />
         </font></Label>
      <Button layoutX="345.0" layoutY="260.0" mnemonicParsing="false" onAction="#handleStatBtn" prefHeight="33.0" prefWidth="167.0" stylesheets="@CSS.css" text="Statistiques" />
