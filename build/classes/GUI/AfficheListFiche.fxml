<AnchorPane id="AnchorPane" prefHeight="409.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.AfficheListFicheController">
   <children>
      <ListView fx:id="fxlistview" layoutX="29.0" layoutY="105.0" onMouseClicked="#handleMouseClicked" prefHeight="200.0" prefWidth="539.0" style="-fx-border-color: blue;" />
      <Button fx:id="fxmodifFiche" layoutX="213.0" layoutY="369.0" mnemonicParsing="false" onAction="#modifierfiche" style="-fx-background-color: blue;" text="modifier" textFill="WHITE">
         <font>
            <Font name="Arial Rounded MT Bold" size="14.0" />
         </font></Button>
      <Button fx:id="fxsuppFiche" layoutX="360.0" layoutY="369.0" mnemonicParsing="false" onAction="#supprimerFiche" style="-fx-background-color: blue;" text="supprimer" textFill="WHITE">
         <font>
            <Font name="Arial Rounded MT Bold" size="14.0" />
         </font></Button>
      <TextField fx:id="fxselectionF" layoutX="26.0" layoutY="322.0" onAction="#selectionFiche" prefHeight="26.0" prefWidth="546.0" style="-fx-border-color: blue;" />
      <Label layoutX="29.0" layoutY="22.0" text="Liste Fiches" textFill="#182dd9">
         <font>
            <Font name="Arial Black" size="24.0" />
         </font>
